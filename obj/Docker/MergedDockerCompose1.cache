services:
  jenkins:
    image: niyazidocker/jenkinswithdocker
    networks:
      default: null
    ports:
    - mode: ingress
      target: 8080
      published: 8080
      protocol: tcp
    - mode: ingress
      target: 50000
      published: 50000
      protocol: tcp
    privileged: true
    user: root
    volumes:
    - type: bind
      source: /Users/niyazibabayev/jenkins
      target: /var/jenkins_home
      bind:
        create_host_path: true
    - type: bind
      source: /var/run/docker.sock
      target: /var/run/docker.sock
      bind:
        create_host_path: true
    - type: bind
      source: /usr/local/bin/docker
      target: /usr/local/bin/docker
      bind:
        create_host_path: true
  mssql:
    environment:
      ACCEPT_EULA: "1"
      MSSQL_SA_PASSWORD: MyPass@word
      MSSQL_USER: sa
    image: mcr.microsoft.com/azure-sql-edge
    networks:
      backend: null
    ports:
    - mode: ingress
      target: 1433
      published: 1433
      protocol: tcp
  sunday_test_api:
    build:
      context: /Users/niyazibabayev/Projects/Sunday_Test_API
      dockerfile: Sunday_Test_API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Sunday_Test_API
      target: base
    container_name: Sunday_Test_API
    depends_on:
      mssql:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      Database: SundayDb
      DbPort: "1433"
      DbServer: mssql
      DbUser: sa
      NUGET_FALLBACK_PACKAGES: ""
      Password: MyPass@word
    image: sundaytestapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Sunday_Test_API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 80
      published: 8090
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: /Users/niyazibabayev/Projects/Sunday_Test_API/Sunday_Test_API
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: /Users/niyazibabayev/vsdbg/vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: /Users/niyazibabayev/.aspnet/https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/niyazibabayev/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/niyazibabayev/.nuget/packages/
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/niyazibabayev/Projects/Sunday_Test_API
      target: /src
      bind:
        create_host_path: true
networks:
  backend:
    name: dockercompose4331238603339208060_backend
    driver: bridge
  default:
    name: dockercompose4331238603339208060_default