services:
  mssql:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: MyPass@word
    image: mcr.microsoft.com/azure-sql-edge
    networks:
      backend: null
    ports:
    - mode: ingress
      target: 1433
      published: 1433
      protocol: tcp
  sunday_test_api:
    build:
      context: /Users/niyazibabayev/Projects/Sunday_Test_API
      dockerfile: Sunday_Test_API/Dockerfile
    depends_on:
      mssql:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      Database: SundayDb
      DbPort: "1433"
      DbServer: mssql
      DbUser: sa
      Password: MyPass@word
    image: sundaytestapi
    networks:
      backend: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 80
      published: 8090
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    volumes:
    - type: bind
      source: /Users/niyazibabayev/.aspnet/https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/niyazibabayev/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
networks:
  backend:
    name: dockercompose4331238603339208060_backend
    driver: bridge